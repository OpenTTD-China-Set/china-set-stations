grf {
    grfid:                  "\5F\5F\03\01";
    name:                   string(STR_GRF_NAME);
    desc:                   string(STR_GRF_DESC);
    url:                    string(STR_GRF_URL);
    version:                42;
    min_compatible_version: 41;
}


// version check
if (ttd_platform != PLATFORM_OPENTTD || openttd_version < version_openttd(12,0)) {
    error(FATAL, string(STR_OPENTTD));
    exit;
}
